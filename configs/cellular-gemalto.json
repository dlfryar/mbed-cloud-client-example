{
  "config": {
    "button-pinname": {
      "help": "PinName for button.",
      "value": "BUTTON3"
    },
    "developer-mode": {
      "help": "Enable Developer mode to skip Factory enrollment",
      "options": [
        null,
        1
      ],
      "value": 1
    },
    "led-pinname": {
      "help": "PinName for led, which is attached to led blink resource.",
      "value": "LED1"
    },
    "mcc-no-auto-format": {
      "help": "If this is null autoformat will occur",
      "value": null
    }
  },
  "macros": [
    "ARM_UC_USE_PAL_BLOCKDEVICE=1",
    "MBED_CLOUD_CLIENT_UPDATE_STORAGE=ARM_UCP_FLASHIAP_BLOCKDEVICE",
    "MBED_CLIENT_DTLS_PEER_MAX_TIMEOUT=100000",
    "PAL_TLS_DEBUG_THRESHOLD=1",
    "MBEDTLS_SSL_DEBUG_ALL=1",
    "CELLULAR_DEVICE=GEMALTO_CINTERION",
    "MDMTXD=D1",
    "MDMRXD=D0",
    "MDMRTS=D3",
    "MDMCTS=D2"
  ],
  "target_overrides": {
    "*": {
      "mbed-client.event-loop-size": 2048,
      "mbed-trace.enable": null,
      "nanostack-hal.event_loop_thread_stack_size": 8192,
      "platform.default-serial-baud-rate": 115200,
      "platform.stdio-baud-rate": 115200,
      "platform.stdio-buffered-serial": true,
      "platform.stdio-convert-newlines": true,
      "target.features_add": [
        "BOOTLOADER",
        "STORAGE",
        "LWIP"
      ],
      "update-client.storage-address": "(1024*1024*64)",
      "update-client.storage-locations": 1,
      "update-client.storage-size": "(1024*1024*2)"
    },
    "NRF52840_DK": {
"nordic.uart_0_fifo_size": 120,
"nordic.uart_1_fifo_size": 120,
      "cellular.debug-at": 1,
      "client_app.primary_partition_size": 1048576,
      "lwip.ethernet-enabled": false,
      "lwip.ipv4-enabled": true,
      "lwip.ipv6-enabled": true,
      "lwip.mem-size": 22000,
      "lwip.ppp-enabled": true,
      "lwip.tcp-enabled": true,
      "nsapi.default-cellular-apn": "\"NXTGENPHONE\"",
      "nsapi.default-cellular-password": null,
      "nsapi.default-cellular-sim-pin": "\"1234\"",
      "nsapi.default-cellular-username": null,
      "target.OUTPUT_EXT": "hex",
      "target.app_offset": "0x3B400",
      "target.bootloader_img": "../tools/mbed-bootloader-NRF52840_DK.hex",
      "target.components_add": [
        "SPIF"
      ],
      "target.features_remove": [
        "CRYPTOCELL310"
      ],
      "target.header_offset": "0x3B000",
      "target.macros_remove": [
        "MBEDTLS_CONFIG_HW_SUPPORT"
      ],
      "target.network-default-interface-type": "CELLULAR",
      "update-client.application-details": "0x3B000",
      "update-client.storage-address": "(1024*1024*1)",
      "update-client.bootloader-details": "0x29C3C",
      "update-client.storage-locations": 1,
      "update-client.storage-size": "(1024*1024*1)"
    }
  }
}
